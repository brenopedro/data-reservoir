<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.ehcache.org/v3"
        xmlns:jsr107="http://www.ehcache.org/v3/jsr107"
        xsi:schemaLocation="
            http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd
            http://www.ehcache.org/v3/jsr107 http://www.ehcache.org/schema/ehcache-107-ext-3.0.xsd">

    <cache alias="cubeCache">
        <key-type>java.lang.String</key-type>
        <value-type>com.reservoir.datareservoir.client.domain.model.CubeData[]</value-type>
        <resources>
            <heap unit="MB">10</heap>
        </resources>
    </cache>
    
    <cache alias="droneCache">
        <key-type>java.lang.String</key-type>
        <value-type>com.reservoir.datareservoir.client.domain.model.DroneData[]</value-type>
        <resources>
            <heap unit="MB">10</heap>
        </resources>
    </cache>
    
    <cache alias="rocketCache">
        <key-type>java.lang.String</key-type>
        <value-type>com.reservoir.datareservoir.client.domain.model.RocketData[]</value-type>
        <resources>
            <heap unit="MB">10</heap>
        </resources>
    </cache>
    
</config>